{
  "compilerOptions": {
    "module": "nodenext",
    "target": "es2022",
    "lib": ["es2022", "dom"],
    "moduleResolution": "nodenext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "strict": false,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "tidyf": ["../../src/raycast-index.ts"],
      "react": ["./node_modules/@types/react"]
    }
  },
  "include": ["src/**/*"]
}
